@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix obo:  <http://purl.obolibrary.org/obo/> .

<http://www.w3.org/ns/prov-bfo-directmappings#>
   a owl:Ontology;
   rdfs:label "Basic Formal Ontology (BFO) mappings for W3C PROVenance Interchange Ontology (PROV-O)"@en;
   owl:imports <https://www.w3.org/ns/prov-o>, 
               <http://purl.obolibrary.org/obo/bfo/2020/bfo.owl> ,
              #  <http://purl.obolibrary.org/obo/ro/releases/2023-02-22/ro.owl> , # TODO only import used terms and their dependents for reasoner
               <https://www.w3.org/TR/prov-o/example-1.ttl> ,
               <https://www.w3.org/TR/prov-o/example-2.ttl> ,
               <https://www.w3.org/TR/prov-o/example-3.ttl> ,
              #  <https://www.w3.org/TR/prov-o/example-4.ttl> ;
              <https://www.w3.org/TR/prov-o/example-5.ttl> ,
              <https://www.w3.org/TR/prov-o/example-7.ttl> ,
              <https://www.w3.org/TR/prov-o/example-8.ttl> ,
              <https://www.w3.org/TR/prov-o/example-9.ttl> ,
              <https://www.w3.org/TR/prov-o/example-10.ttl> ,
              <https://www.w3.org/TR/prov-o/example-11.ttl> ;
   owl:versionInfo "2023-07-18"@en;
   owl:versionIRI <http://www.w3.org/ns/prov-bfo-directmappings-20230718>;
   rdfs:seeAlso        <http://www.w3.org/ns/prov>;
.

# Classes
prov:Entity               rdfs:subClassOf obo:BFO_0000002 . # continuant
prov:Agent                rdfs:subClassOf obo:BFO_0000040 . # material entity
prov:Activity             rdfs:subClassOf obo:BFO_0000015 . # process
prov:Role                 rdfs:subClassOf obo:BFO_0000034 . # function
#prov:AgentRole           rdfs:subClassOf obo:BFO_0000023 . # role
prov:Location             rdfs:subClassOf obo:BFO_0000029 . # site
prov:InstantaneousEvent   rdfs:subClassOf obo:BFO_0000035 . # process boundary


# Object Properties
prov:influenced           rdfs:subPropertyOf obo:RO_0002410 .  # causally related to
prov:wasAttributedTo      rdfs:subPropertyOf obo:RO_0002559 .  # causally influenced by # TODO inconsistent with example-4:  <:publicationActivity1124, prov:wasAttributedTo, :postEditor> because Domain is Entity (occurrent not continuant)
prov:alternateOf          rdfs:subPropertyOf obo:RO_0002559 .  # causally influenced by
prov:wasDerivedFrom       rdfs:subPropertyOf obo:RO_0002559 .  # causally influenced by

prov:wasEndedBy           rdfs:subPropertyOf obo:RO_0000057 .  # has participant
prov:wasStartedBy         rdfs:subPropertyOf obo:RO_0000057 .  # has participant
prov:wasAssociatedWith    rdfs:subPropertyOf obo:RO_0000057 .  # has participant
prov:invalidated          rdfs:subPropertyOf obo:RO_0000057 .  # has participant
prov:used                 rdfs:subPropertyOf obo:RO_0000057 .  # has participant
prov:generated            rdfs:subPropertyOf obo:RO_0000057 .  # has participant

prov:wasInvalidatedBy     rdfs:subPropertyOf obo:RO_0000056 .  # participates in
prov:wasGeneratedBy       rdfs:subPropertyOf obo:RO_0000056 .  # participates in

prov:wasInformedBy            rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:hadGeneration            rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:hadUsage                 rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:qualifiedAssociation     rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:qualifiedCommunication   rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:qualifiedStart           rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes

prov:hadMember                rdfs:subPropertyOf obo:RO_0002351 .  # has member