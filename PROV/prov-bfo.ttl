@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix obo:  <http://purl.obolibrary.org/obo/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .

<http://www.w3.org/ns/prov-bfo-directmappings#>
   a owl:Ontology;
   rdfs:label "Basic Formal Ontology (BFO) mappings for W3C PROVenance Interchange Ontology (PROV-O)"@en;
   owl:imports <https://www.w3.org/ns/prov-o>, 
               <http://purl.obolibrary.org/obo/bfo/2020/bfo.owl> ,
              #  <http://purl.obolibrary.org/obo/ro/releases/2023-02-22/ro.owl> ; # TODO only import used terms and their dependents for reasoner
               <https://www.w3.org/TR/prov-o/example-1.ttl> ,
               <https://www.w3.org/TR/prov-o/example-2.ttl> ,
               <https://www.w3.org/TR/prov-o/example-3.ttl> ,
               <https://www.w3.org/TR/prov-o/example-4.ttl> ;
   owl:versionInfo "2013-04-30"@en; # todo fix
   owl:versionIRI <http://www.w3.org/ns/prov-dc-directmappings-20130430>; # TODO fix
   rdfs:seeAlso        <http://www.w3.org/TR/2013/NOTE-prov-dc-20130430/>, # TODO
                       <http://www.w3.org/ns/prov>;
  #prov:wasRevisionOf <http://www.w3.org/ns/prov-dc-directmappings-20130430>; # TODO 
.

prov:Entity rdfs:subClassOf obo:BFO_0000002 . # continuant
prov:Agent rdfs:subClassOf obo:BFO_0000040 . # material entity
prov:Activity rdfs:subClassOf obo:BFO_0000015 . # process
prov:Role rdfs:subClassOf obo:BFO_0000034 . # function
#prov:AgentRole rdfs:subClassOf obo:BFO_0000023 . # role
prov:Location rdfs:subClassOf obo:BFO_0000029 . # site
prov:InstantaneousEvent rdfs:subClassOf obo:BFO_0000035 . # process boundary

prov:wasAssociatedWith rdfs:subPropertyOf obo:RO_0000057 . #
prov:wasDerivedFrom rdfs:subPropertyOf obo:RO_0002559 . # 
prov:wasInvalidatedBy rdfs:subPropertyOf obo:RO_0000056. #
prov:generated rdfs:subPropertyOf obo:RO_0000057 . #
prov:wasGeneratedBy rdfs:subPropertyOf obo:RO_0000056 . # participates in
prov:influenced rdfs:subPropertyOf obo:RO_0002410 .  # causally related to
# TODO inconsistent with example-4:  <:publicationActivity1124, prov:wasAttributedTo, :postEditor>
prov:wasAttributedTo rdfs:subPropertyOf obo:RO_0002559 .  # causally influenced by
prov:wasInformedBy rdfs:subPropertyOf obo:RO_0002501 .  # causal relation between processes
prov:invalidated rdfs:subPropertyOf obo:RO_00000573 .  # has participant
prov:hadMember rdfs:subPropertyOf obo:RO_0002351 .  # has member 
prov:alternateOf rdfs:subPropertyOf obo:RO_0002559 .  # causally influenced by