@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix obo:  <http://purl.obolibrary.org/obo/> .
@prefix cco: <http://www.ontologyrepository.com/CommonCoreOntologies/> .
@base <https://raw.githubusercontent.com/BFO-Mappings/BFO-Mappings/main/PROV/prov-cco-directmappings.ttl> .

<https://raw.githubusercontent.com/BFO-Mappings/BFO-Mappings/main/PROV/prov-cco-directmappings.ttl>
   a                     owl:Ontology;
   rdfs:label           "Common Core Ontologies (CCO) mappings for W3C PROVenance Interchange Ontology (PROV-O)"@en ;
   owl:versionInfo      "2023-07-19"@en ;
   owl:versionIRI       <http://www.w3.org/ns/prov-cco-directmappings-20230718> ;
   owl:imports          <https://raw.githubusercontent.com/BFO-Mappings/BFO-Mappings/main/PROV/prov-bfo-directmappings.ttl> ;
   rdfs:seeAlso         <http://www.w3.org/ns/prov> ,
                        <https://github.com/CommonCoreOntology/CommonCoreOntologies> ;
.

# Class mappings
# prov:Agent                  rdfs:subClassOf cco:Agent .
prov:Activity                 rdfs:subClassOf cco:Act .
prov:Plan                     rdfs:subClassOf cco:PlanSpecification .
prov:Start                    rdfs:subClassOf cco:ProcessBeginning .
prov:End                      rdfs:subClassOf cco:ProcessEnding .


# Object Property mappings
# prov:influenced           rdfs:subPropertyOf obo:RO_0002410 .
# prov:wasAttributedTo      rdfs:subPropertyOf obo:RO_0002559 .
# prov:alternateOf          rdfs:subPropertyOf obo:RO_0002559 .
# prov:wasDerivedFrom       rdfs:subPropertyOf obo:RO_0002559 . 

prov:wasEndedBy           rdfs:subPropertyOf cco:affects .
prov:wasStartedBy         rdfs:subPropertyOf cco:affects .
prov:wasAssociatedWith    rdfs:subPropertyOf cco:has_agent .
# prov:invalidated          rdfs:subPropertyOf obo:RO_0000057 .
# prov:used                 rdfs:subPropertyOf obo:RO_0000057 .
# prov:generated            rdfs:subPropertyOf obo:RO_0000057 .

prov:wasInvalidatedBy     rdfs:subPropertyOf cco:is_affected_by .
prov:wasGeneratedBy       rdfs:subPropertyOf obo:is_output_of .

# prov:wasInformedBy            rdfs:subPropertyOf obo:RO_0002501 .
# prov:hadGeneration            rdfs:subPropertyOf obo:RO_0002501 .
# prov:hadUsage                 rdfs:subPropertyOf obo:RO_0002501 .
# prov:qualifiedAssociation     rdfs:subPropertyOf obo:RO_0002501 .
# prov:qualifiedCommunication   rdfs:subPropertyOf obo:RO_0002501 .
# prov:qualifiedStart           rdfs:subPropertyOf obo:RO_0002501 .

# prov:hadMember                rdfs:subPropertyOf obo:RO_0002351 .



prov:Activity  rdfs:subClassOf   [ rdf:type owl:Restriction ;
                                    owl:onProperty cco:has_agent ;
                                    owl:someValuesFrom prov:Agent .
                                 ] .